        "async-mutex": "^0.5.0",
        "echarts": "^6.0.0",
        "echarts-for-react": "^3.0.4",
        "i18next": "^25.6.0",
        "i18next-browser-languagedetector": "^8.2.0",
        "lucide-react": "^0.546.0",
        "react": "^19.2.0",
        "react-dom": "^19.2.0",
        "react-error-boundary": "^6.0.0",
        "react-hook-form": "^7.65.0",
